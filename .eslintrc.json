{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["error", "never"],
        "react/jsx-uses-semi": ["error", "never"],
        "jsx-quotes": ["error", "prefer-single"],
        "quotes": ["error", "single"]
    }
}
